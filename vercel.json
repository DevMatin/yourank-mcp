{
  "version": 2,
  "builds": [
    {
      "src": "src/main/index-sse-http.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/main/index-sse-http.ts"
    }
  ],
  "crons": [
    { "path": "/api/cron/cleanup-blobs", "schedule": "0 3 * * *" }
  ]
} 