          "purpose": "Performance-Waterfall-Analyse",
          "required_params": ["id", "url"],
          "use_cases": [
            "Ladezeiten analysieren",
            "Performance-Bottlenecks identifizieren",
            "Optimierungsempfehlungen"
          ],
          "example_prompts": [
            "Analysiere die Ladezeit der Startseite",
            "Welche Ressourcen verlangsamen die Seite am meisten?",
            "Zeige mir das Performance-Waterfall-Diagramm"
          ]
        },
        "keyword_density": {
          "purpose": "Keyword-Dichte und -Häufigkeit analysieren",
          "required_params": ["id"],
          "optional_params": ["url"],
          "use_cases": [
            "SEO-Keyword-Optimierung",
            "Content-Analyse",
            "Keyword-Stuffing erkennen"
          ],
          "example_prompts": [
            "Analysiere die Keyword-Dichte auf der Startseite",
            "Welche Keywords werden am häufigsten verwendet?",
            "Finde Keyword-Stuffing-Probleme"
