  "common_parameters": {
    "target": {
      "type": "string",
      "description": "URL oder Domain der zu analysierenden Website",
      "examples": ["example.com", "https://www.example.com", "shop.example.com"],
      "required_for": ["task_post", "instant_pages", "content_parsing_live"]
    },
    "id": {
      "type": "string", 
      "description": "Task ID von vorheriger Analyse",
      "examples": ["12345-67890-abcdef", "task_12345"],
      "required_for": ["summary", "pages", "resources", "links", "duplicate_content", "duplicate_tags", "waterfall", "keyword_density", "raw_html", "page_screenshot", "content_parsing", "lighthouse_task_get", "force_stop", "pages_by_resource", "redirect_chains", "non_indexable"]
    },
    "url": {
      "type": "string",
      "description": "Spezifische URL für Analyse",
      "examples": ["https://example.com/page", "https://shop.example.com/product/123"],
      "required_for": ["waterfall", "keyword_density", "raw_html", "page_screenshot", "content_parsing", "content_parsing_live", "instant_pages", "microdata", "lighthouse_task_post", "lighthouse_live"]
    },
    "limit": {
      "type": "integer",
      "description": "Anzahl der zurückgegebenen Ergebnisse",
      "default": 10,
      "min": 1,
      "max": 1000,
      "use_cases": ["Pagination", "Ergebnisbegrenzung", "Performance-Optimierung"]
    },
    "offset": {
      "type": "integer", 
      "description": "Offset für Pagination",
      "default": 0,
      "min": 0,
      "use_cases": ["Pagination", "Große Datensätze durchsuchen"]
    },
    "enable_javascript": {
      "type": "boolean",
      "description": "JavaScript-Ausführung aktivieren",
      "default": false,
      "cost_impact": "Zusätzliche Kosten",
      "use_cases": ["SPA-Analyse", "Dynamischer Content", "JavaScript-basierte Features"]
    },
    "custom_js": {
      "type": "string",
      "description": "Custom JavaScript Code ausführen",
      "cost_impact": "Zusätzliche Kosten",
      "use_cases": ["Custom Interaktionen", "Spezielle Analysen", "Dynamische Tests"]
    },
    "custom_user_agent": {
      "type": "string",
      "description": "Custom User-Agent Header",
      "default": "Mozilla/5.0 (compatible; RSiteAuditor)",
      "use_cases": ["Mobile-Simulation", "Bot-Erkennung umgehen", "Spezielle Browser-Simulation"]
    },
    "accept_language": {
      "type": "string",
      "description": "Accept-Language Header",
      "default": "en-US,en;q=0.9",
      "use_cases": ["Sprach-spezifische Inhalte", "Lokalisierung", "Internationale Websites"]
    },
    "enable_browser_rendering": {
      "type": "boolean",
      "description": "Browser-Rendering für Core Web Vitals",
      "default": false,
      "cost_impact": "Zusätzliche Kosten",
      "use_cases": ["Performance-Messung", "Core Web Vitals", "Echte Browser-Metriken"]
    },
    "load_resources": {
      "type": "boolean",
      "description": "Ressourcen (CSS, JS, Bilder) laden",
      "default": true,
      "cost_impact": "Zusätzliche Kosten",
      "use_cases": ["Vollständige Seiten-Analyse", "Performance-Tests", "Resource-Analyse"]
    },
    "max_crawl_pages": {
      "type": "integer",
      "description": "Maximale Anzahl zu crawlender Seiten",
      "default": 10,
      "min": 1,
      "max": 1000,
      "use_cases": ["Crawl-Begrenzung", "Kostenkontrolle", "Fokus-Analyse"]
    },
    "screenshot_width": {
      "type": "integer",
      "description": "Screenshot-Breite in Pixeln",
      "default": 1920,
      "min": 320,
      "max": 3840,
      "use_cases": ["Desktop-Screenshots", "Hochauflösende Bilder", "Spezielle Auflösungen"]
    },
    "screenshot_height": {
      "type": "integer", 
      "description": "Screenshot-Höhe in Pixeln",
      "default": 1080,
      "min": 240,
      "max": 2160,
      "use_cases": ["Desktop-Screenshots", "Hochauflösende Bilder", "Spezielle Auflösungen"]
    },
    "viewport_width": {
      "type": "integer",
      "description": "Viewport-Breite für Rendering",
      "default": 1920,
      "min": 320,
      "max": 3840,
      "use_cases": ["Mobile-Simulation", "Responsive Design Tests", "Viewport-spezifische Analysen"]
    },
    "viewport_height": {
      "type": "integer",
      "description": "Viewport-Höhe für Rendering", 
      "default": 1080,
      "min": 240,
      "max": 2160,
      "use_cases": ["Mobile-Simulation", "Responsive Design Tests", "Viewport-spezifische Analysen"]
    },
    "language_code": {
      "type": "string",
      "description": "Sprachcode für Analyse",
      "default": "en",
      "examples": ["en", "de", "fr", "es", "it"],
      "use_cases": ["Lokalisierte Analysen", "Sprach-spezifische SEO", "Internationale Optimierung"]
    },
    "location_name": {
      "type": "string",
      "description": "Geografischer Standort",
      "default": "United States",
      "examples": ["United States", "Germany", "United Kingdom", "France"],
      "use_cases": ["Lokale SEO", "Geografische Analysen", "Standort-spezifische Ergebnisse"]
    },
    "device": {
      "type": "string",
      "description": "Gerätetyp für Analyse",
      "enum": ["desktop", "mobile"],
      "default": "desktop",
      "use_cases": ["Mobile vs Desktop", "Responsive Design", "Geräte-spezifische Optimierung"]
    },
    "category": {
      "type": "array",
      "description": "Lighthouse-Kategorien",
      "items": {
        "enum": ["performance", "accessibility", "seo", "pwa", "best-practices"]
      },
      "use_cases": ["Spezifische Lighthouse-Tests", "Fokussierte Analysen", "Kategorie-spezifische Optimierung"]
    },
    "filters": {
      "type": "array",
      "description": "Filter für Ergebnisse",
      "items": {"type": "object"},
      "use_cases": ["Ergebnis-Filterung", "Spezifische Analysen", "Daten-Einschränkung"]
    },
    "order_by": {
      "type": "array",
      "description": "Sortierung der Ergebnisse",
      "items": {"type": "string"},
      "use_cases": ["Ergebnis-Sortierung", "Priorisierung", "Daten-Organisation"]
    },
    "summary_only": {
      "type": "boolean",
      "description": "Nur Zusammenfassung ohne Details",
      "default": true,
      "use_cases": ["Schnelle Übersichten", "Token-Optimierung", "Performance-Verbesserung"]
    }
  },
