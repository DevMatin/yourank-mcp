      "operations": {
        "pages": {
          "purpose": "Analysiere alle gecrawlten Seiten",
          "required_params": ["id"],
          "optional_params": ["url", "limit", "offset", "filters", "order_by"],
          "use_cases": [
            "Seiten-Übersicht mit SEO-Metriken",
            "Performance-Probleme identifizieren",
            "Content-Qualität bewerten"
          ],
          "example_prompts": [
            "Analysiere alle Seiten der Website mit den schlechtesten SEO-Scores",
            "Zeige mir die 20 langsamsten Seiten",
            "Welche Seiten haben die meisten OnPage-Issues?"
