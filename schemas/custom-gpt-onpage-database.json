  "endpoints": {
    "core": {
      "path": "/v3/onpage_core",
      "description": "Kernfunktionen für Task-Management und Website-Crawling",
      "operations": {
        "task_post": {
          "purpose": "Erstelle neue OnPage-Analyse-Tasks",
          "required_params": ["target"],
          "optional_params": [
            "max_crawl_pages", "start_url", "enable_javascript", "custom_js",
            "custom_user_agent", "accept_language", "enable_browser_rendering",
            "load_resources", "custom_settings", "language_code", "location_name", "device"
          ],
          "use_cases": [
            "Website-Crawling starten",
            "SEO-Analyse initialisieren", 
            "Performance-Tests einleiten",
            "Content-Audit vorbereiten"
          ],
          "example_prompts": [
            "Analysiere die Website example.com mit JavaScript-Aktivierung",
            "Starte ein Crawling von 50 Seiten der Domain shop.example.com",
            "Führe eine OnPage-Analyse mit Browser-Rendering durch"
          ]
        },
        "summary": {
          "purpose": "Hole Zusammenfassung der OnPage-Analyse",
          "required_params": ["id"],
          "use_cases": [
            "Crawling-Status prüfen",
            "Gesamtübersicht der gefundenen Issues",
            "Fortschritt der Analyse verfolgen"
          ],
          "example_prompts": [
            "Zeige mir die Zusammenfassung der Analyse für Task ID 12345",
            "Wie ist der Status des Website-Crawlings?",
            "Welche OnPage-Probleme wurden gefunden?"
          ]
        },
        "tasks_ready": {
          "purpose": "Liste aller fertigen Tasks abrufen",
          "use_cases": [
            "Verfügbare Analysen anzeigen",
            "Task-Status überwachen",
            "Fertige Ergebnisse identifizieren"
          ],
          "example_prompts": [
            "Welche OnPage-Analysen sind fertig?",
            "Zeige mir alle verfügbaren Task-Ergebnisse",
            "Liste die abgeschlossenen Website-Analysen auf"
          ]
        }
      }
    },
    "analysis": {
