        "resources": {
          "purpose": "Analysiere Website-Ressourcen (CSS, JS, Bilder)",
          "required_params": ["id"],
          "use_cases": [
            "Performance-Bottlenecks finden",
            "Broken Resources identifizieren",
            "Optimierungsmöglichkeiten aufzeigen"
