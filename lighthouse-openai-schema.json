    "/v3/on_page/lighthouse/task_get/json/{id}": {
      "get": {
        "tags": ["Lighthouse"],
        "summary": "Lighthouse Ergebnisse abrufen",
        "description": "Abrufen der Lighthouse Test Ergebnisse anhand der Task-ID. Bei großen Responses werden die Daten in Blob Storage gespeichert.",
        "operationId": "getLighthouseResults",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "Task-ID des Lighthouse Tests",
            "schema": {
              "type": "string",
              "format": "uuid"
            },
            "example": "07131248-1535-0216-1000-17384017ad04"
          }
        ],
        "responses": {
          "200": {
            "description": "Erfolgreiche Operation - Lighthouse Ergebnisse oder Blob Storage Referenz",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/LighthouseResults"
                    },
                    {
                      "$ref": "#/components/schemas/LighthouseBlobResponse"
                    }
                  ]
                }
              }
            }
          }
        },
        "security": [{"basicAuth": []}]
      }
    },
